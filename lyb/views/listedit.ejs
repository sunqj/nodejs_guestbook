<!DOCTYPE html>
<html>
<head>
    <title>编辑接口</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<h1>编辑</h1>
<% if (lists.length) {

%>
<form action="listedit" method="post" >
    <% lists.forEach(function(vo, index){  %>
    <input type="hidden" id="id" name="id" value="<%=id%>" />
    接口名称：<input type="text" name="name" id="name" value="<%=vo.name%>"  style="width:200px;" /><br />
    内容：
    <textarea rows="30" cols="80" name="content" id="content" ><%=vo.content%></textarea><br />
    <input type="submit" name="addsubmit" id="addsubmit" value="提交" />
    <% }) %>
</form>
<% } %>

</body>
</html>
